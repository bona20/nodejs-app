pipeline{
    agent any

    stages{
        stage ('Dep install'){
            agent { docker { image 'node:9.8.0'
            args '-u root:sudo'}}
            steps{
                // sh 'npm cash clean'
               //  sh 'sudo chown -R 995:993 "/.npm"'
                sh 'npm install'
            }
        }
    }
}